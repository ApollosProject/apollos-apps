{
  "name": "@apollosproject/ui-kit",
  "version": "0.8.0-alpha.4",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "src/index.js",
  "files": [
    "src/**"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "transform": {
      ".+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-linear-gradient": "*",
    "react-native-svg": "*"
  },
  "dependencies": {
    "color": "^3.1.0",
    "lodash": "^4.17.11",
    "react-native-modal-datetime-picker": "6.0.0",
    "react-native-tab-view": "1.0.2",
    "recompose": "^0.30.0",
    "rn-placeholder": "1.2.0"
  },
  "devDependencies": {
    "@apollosproject/babel-preset-apollos": "^0.8.0-alpha.4",
    "@storybook/react-native": "^4.0.7",
    "codecov": "3.1.0",
    "jest": "23.6.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "16.6.1",
    "react-dom": "16.6.3",
    "react-native": "0.57.5",
    "react-native-linear-gradient": "2.4.0",
    "react-native-svg": "6.5.2",
    "react-navigation": "2.11.2",
    "react-test-renderer": "16.6.1"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "375e9a2d747fe4c0b5406681e2db676d60c24227"
}

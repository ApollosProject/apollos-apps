{
  "presets": [
    "babel-preset-react-native-stage-0/decorator-support",
    "react-native-dotenv"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".android.js", ".ios.js", ".js"]
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  }
}

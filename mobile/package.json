{
  "name": "apollos-church-app",
  "version": "0.3.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.0.0-beta.47",
    "@babel/parser": "^7.0.0-beta.54",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.47",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.47",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react-native": "^3.4.8",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-plugin-import-graphql": "^2.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "^5.0.2",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-native": "^3.2.1",
    "formik": "^0.11.11",
    "jest": "^23.4.1",
    "jest-react-native": "^18.0.0",
    "nodemon": "^1.17.5",
    "prettier": "1.12.1",
    "prop-types": "^15.6.1",
    "react-dom": "16.3.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-storybook-loader": "^1.8.0",
    "react-native-version": "^2.6.2",
    "react-test-renderer": "^16.4.1",
    "waait": "^1.0.2",
    "yup": "^0.25.1"
  },
  "engines": {
    "node": "^8.11.x"
  },
  "scripts": {
    "android": "react-native run-android",
    "codecov": "cat ./coverage/lcov.info | ./node_modules/.bin/codecov",
    "fixlint": "./node_modules/.bin/eslint . --fix",
    "ios": "react-native run-ios --simulator=\"iPhone X\"",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "postversion":
      "react-native-version --never-amend --android android/app/build.gradle --ios ios/",
    "start": "node ./scripts/get-introspection-data.js && react-native start",
    "storybook":
      "rnstl --searchDir ./storybook ./src --pattern \"**/*.stories.js\" && storybook start -p 7007",
    "test": "bash ../scripts/test-script.sh",
    "watch-graphql":
      "./node_modules/.bin/nodemon --exec \"rm -rf\" ./node_modules/.cache/babel-loader --watch src -e .graphql -q"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation))"
    ],
    "testMatch": ["<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"],
    "modulePaths": ["<rootDir>/src/"],
    "setupFiles": ["./jest.setup.js"],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.3.2",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "apollo-link-schema": "^1.1.0",
    "apollo-link-state": "^0.4.1",
    "color": "^3.0.0",
    "entities": "^1.1.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.5",
    "htmlparser2": "^3.9.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "react": "16.4.1",
    "react-apollo": "^2.1.4",
    "react-native": "~0.56.0",
    "react-native-custom-tabs": "^0.1.7",
    "react-native-keyboard-aware-scroll-view": "^0.6.0",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-modal-datetime-picker": "^6.0.0",
    "react-native-safari-view": "^2.1.0",
    "react-native-sentry": "^0.38.1",
    "react-native-splash-screen": "3.0.6",
    "react-native-svg": "^6.3.1",
    "react-native-tab-view": "^1.0.2",
    "react-navigation": "^2.7.0",
    "recompose": "^0.27.1",
    "rn-placeholder": "^1.2.0",
    "stream": "^0.0.2"
  },
  "rnpm": {
    "assets": ["assets/fonts"]
  }
}

{
  "name": "@apollosproject/ui-connected",
  "version": "1.3.0",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "test": "jest",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@apollosproject|@react-navigation|@react-native-community/async-storage))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "peerDependencies": {
    "@apollosproject/config": "*",
    "@apollosproject/ui-analytics": "*",
    "@apollosproject/ui-auth": "*",
    "@apollosproject/ui-fragments": "*",
    "@apollosproject/ui-htmlview": "*",
    "@apollosproject/ui-kit": "*",
    "@apollosproject/ui-media-player": "*",
    "@apollosproject/ui-storybook": "*",
    "@react-native-community/async-storage": "*",
    "react": "*",
    "react-native": "*",
    "react-native-image-picker": "*",
    "react-native-inappbrowser-reborn": "*",
    "react-native-permissions": "*",
    "react-navigation": "*"
  },
  "dependencies": {
    "apollo-upload-client": "9.0.0",
    "graphql-tag": "2.9.2",
    "lodash": "^4.17.11",
    "react-apollo": "2.5.5"
  },
  "devDependencies": {
    "@react-native-community/async-storage": "^1.5.1",
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-image-picker": "0.28.0",
    "react-native-inappbrowser-reborn": "3.3.4",
    "react-native-permissions": "2.0.5",
    "react-native-webview": "^7.0.0",
    "react-navigation": "^3.11.0",
    "react-test-renderer": "16.6.3",
    "waait": "1.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md"
}

{
  "name": "apollos-church-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/addons": "^3.4.5",
    "@storybook/react-native": "^3.4.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^23.0.0",
    "jest-react-native": "^18.0.0",
    "prettier": "1.12.1",
    "prop-types": "^15.6.1",
    "react-dom": "16.3.1",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.3.1"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "yarn run lint && jest",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "fixlint": "./node_modules/.bin/eslint . --fix",
    "storybook":
      "rnstl --searchDir ./storybook ./src --pattern \"**/*.stories.js\" && storybook start -p 7007"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": ["<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"],
    "modulePaths": ["<rootDir>/src/"]
  },
  "dependencies": {
    "color": "^3.0.0",
    "lodash": "^4.17.10",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "recompose": "^0.27.1"
  }
}

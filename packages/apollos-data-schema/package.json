{
  "name": "@apollosproject/data-schema",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "version": "1.7.0-beta.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "test": "jest",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "graphql-tag": "^2.10.0"
  },
  "devDependencies": {
    "@apollosproject/babel-preset-apollos": "^1.7.0-beta.0",
    "jest": "^23.6.0"
  },
  "peerDependencies": {
    "@apollosproject/config": "*"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

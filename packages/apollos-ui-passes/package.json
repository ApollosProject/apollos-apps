{
  "name": "@apollosproject/ui-passes",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "version": "1.8.0-beta.0",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "test": "jest --no-watchman",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@apollosproject))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "modulePathIgnorePatterns": [
      "node_modules/@storybook/"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ]
  },
  "peerDependencies": {
    "@apollosproject/ui-kit": "*",
    "react": "*",
    "react-native": "*",
    "react-native-passkit-wallet": "*",
    "react-navigation": "*",
    "rn-fetch-blob": "*"
  },
  "dependencies": {
    "@apollosproject/ui-kit": "^1.8.0-beta.0",
    "graphql-tag": "^2.10.1",
    "react-apollo": "2.5.5"
  },
  "devDependencies": {
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-passkit-wallet": "^0.1.4",
    "react-test-renderer": "16.8.6",
    "rn-fetch-blob": "^0.10.15"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

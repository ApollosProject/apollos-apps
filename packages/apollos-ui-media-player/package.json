{
  "name": "@apollosproject/ui-media-player",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "version": "1.8.0-beta.0",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "test": "jest --no-watchman",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@apollosproject))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "modulePathIgnorePatterns": [
      "node_modules/@storybook/"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "peerDependencies": {
    "@apollosproject/react-native-airplay-btn": "*",
    "@apollosproject/ui-kit": "*",
    "apollo-client": "*",
    "react": "*",
    "react-apollo": "*",
    "react-native": "*",
    "react-native-linear-gradient": "*",
    "react-native-music-control": "*",
    "react-native-video": "*",
    "react-native-video-controls": "*",
    "react-navigation": "*"
  },
  "dependencies": {
    "graphql-tag": "^2.10.1"
  },
  "devDependencies": {
    "@apollosproject/react-native-airplay-btn": "^0.2.0",
    "@storybook/react-native": "5.2.4",
    "apollo-client": "2.6.8",
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "16.8.6",
    "react-apollo": "2.5.5",
    "react-native": "0.60.5",
    "react-native-google-cast": "^3.3.0",
    "react-native-linear-gradient": "2.5.3",
    "react-native-music-control": "0.10.8",
    "react-native-svg": "9.4.0",
    "react-native-video": "5.0.0",
    "react-native-video-controls": "2.2.3",
    "react-navigation": "^3.11.0",
    "react-test-renderer": "16.8.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

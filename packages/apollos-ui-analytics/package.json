{
  "name": "@apollosproject/ui-analytics",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "version": "2.0.0-beta.0",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "test": "jest --no-watchman",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcovonly",
      "clover"
    ],
    "transformIgnorePatterns": [
      "../../node_modules/(?!((jest-)?react-native|rn-*|@react-native-picker|react-clone-referenced-element|@react-navigation/native|@apollosproject|@react-navigation/stack))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "../../node_modules/react-native-gesture-handler/jestSetup.js",
      "./jest.setup.js"
    ]
  },
  "peerDependencies": {
    "@apollo/client": "*",
    "@apollosproject/ui-kit": "*",
    "@metarouter/analytics-react-native": "*",
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {
    "graphql-tag": "2.9.2",
    "react-native-device-info": "^4.0.1"
  },
  "devDependencies": {
    "@apollo/client": "3.3.0-rc.2",
    "@metarouter/analytics-react-native": "^1.1.5",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^2.1.2",
    "prop-types": "15.6.1",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-gesture-handler": "^1.3.0",
    "react-test-renderer": "16.13.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

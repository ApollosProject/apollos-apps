{
  "name": "@apollosproject/ui-analytics",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "version": "1.8.0-beta.0",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "test": "jest --no-watchman",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@apollosproject))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "peerDependencies": {
    "@apollosproject/ui-kit": "*",
    "@metarouter/analytics-react-native": "*",
    "apollo-client": "*",
    "react": "*",
    "react-apollo": "*",
    "react-native": "*",
    "react-navigation": "*"
  },
  "dependencies": {
    "graphql-tag": "2.9.2",
    "react-native-device-info": "^4.0.1"
  },
  "devDependencies": {
    "@metarouter/analytics-react-native": "^1.1.5",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "2.6.8",
    "apollo-link-http": "^1.5.14",
    "jest": "24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "16.8.6",
    "react-apollo": "2.5.5",
    "react-native": "0.60.5",
    "react-test-renderer": "16.8.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

{
  "name": "@apollosproject/ui-mapview",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "engines": {
    "node": ">=12"
  },
  "version": "2.20.0",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "test": "jest --no-watchman",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcovonly",
      "clover"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|@react-native-picker|react-clone-referenced-element|@react-navigation/native|@react-navigation/stack|@apollosproject|@react-native-picker))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "../apollos-ui-test-utils/src/mocks/reanimated.js",
      "./jest.setup.js"
    ]
  },
  "peerDependencies": {
    "@apollosproject/config": "*",
    "@apollosproject/storybook": "*",
    "@apollosproject/ui-fragments": "*",
    "@apollosproject/ui-kit": "*",
    "@apollosproject/ui-media-player": "*",
    "react": "*",
    "react-native": "*",
    "react-native-geolocation-service": "*",
    "react-native-maps": "*",
    "react-native-safe-area-context": "*"
  },
  "dependencies": {
    "graphql-tag": "2.9.2"
  },
  "devDependencies": {
    "@react-native-community/geolocation": "^2.0.2",
    "jest": "^25.1.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-geolocation-service": "^5.1.1",
    "react-native-maps": "0.26.1",
    "react-native-safe-area-context": "^3.0.7",
    "react-test-renderer": "16.13.1",
    "waait": "1.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

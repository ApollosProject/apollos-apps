{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "stage-0"
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    }
  },
  "plugins": [
    [
      "module-resolver", {
        "root": ["../api/src"],
        "cwd": "packagejson",
        "extensions": [".android.js", ".ios.js", ".js", ".graphql"],
        "alias": {
          "/api": "./src"
        }
      }
    ]
  ]
}

{
  "name": "@apollosproject/data-connector-passes",
  "version": "1.3.0",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "reporters": [
      "<rootDir>/../../scripts/fingers-crossed-reporter.js"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@apollosproject/data-connector-rock": "^1.3.0",
    "@apollosproject/data-schema": "^1.3.0",
    "@apollosproject/server-core": "^1.3.0",
    "apollo-datasource": "^0.4.0",
    "datauri": "^1.1.0",
    "lodash": "^4.17.11",
    "passkit-generator": "1.5.9",
    "qrcode": "^1.3.3"
  },
  "peerDependencies": {
    "@apollosproject/config": "*",
    "apollo-server": "^2.5.0"
  },
  "devDependencies": {
    "@apollosproject/apollo-server-env-mock": "^1.3.0",
    "@apollosproject/babel-preset-apollos": "^1.3.0",
    "apollo-server-env": "^2.3.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.1",
    "jest": "^24.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

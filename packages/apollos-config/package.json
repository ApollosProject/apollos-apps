{
  "name": "@apollosproject/config",
  "version": "1.2.0-beta.1",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "reporters": [
      "<rootDir>/../../scripts/fingers-crossed-reporter.js"
    ]
  },
  "devDependencies": {
    "@apollosproject/babel-preset-apollos": "^1.2.0-beta.1"
  },
  "dependencies": {
    "js-yaml": "^3.12.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

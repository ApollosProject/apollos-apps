{
  "name": "@apollosproject/ui-auth",
  "version": "1.5.0-beta.0",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "test": "jest --no-watchman",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@apollosproject|@react-navigation))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "peerDependencies": {
    "@apollosproject/ui-analytics": "*",
    "@apollosproject/ui-kit": "*",
    "@apollosproject/ui-notifications": "*",
    "@apollosproject/ui-storybook": "*",
    "@react-native-community/async-storage": "*",
    "apollo-client": "*",
    "react": "*",
    "react-apollo": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-navigation": "*"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-link-context": "1.0.14",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.14",
    "formik": "1.5.1",
    "graphql-tag": "2.9.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@react-native-community/async-storage": "^1.5.1",
    "apollo-client": "2.6.8",
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "^16.8.3",
    "react-apollo": "2.5.5",
    "react-native": "0.60.5",
    "react-native-gesture-handler": "^1.3.0",
    "react-navigation": "^3.11.0",
    "react-test-renderer": "16.8.6",
    "waait": "^1.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

{
  "name": "@apollosproject/data-connector-passes",
  "version": "0.8.0-alpha.9",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "reporters": [
      "<rootDir>/../../scripts/fingers-crossed-reporter.js"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@apollosproject/data-schema": "^0.8.0-alpha.9",
    "@apollosproject/server-core": "^0.8.0-alpha.9",
    "datauri": "^1.1.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "passkit-generator": "alexandercerutti/passkit-generator#next",
    "qrcode": "^1.3.3"
  },
  "peerDependencies": {
    "@apollosproject/config": "*"
  },
  "devDependencies": {
    "@apollosproject/apollo-server-env-mock": "^0.8.0-alpha.9",
    "@apollosproject/babel-preset-apollos": "^0.8.0-alpha.9",
    "jest": "^23.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

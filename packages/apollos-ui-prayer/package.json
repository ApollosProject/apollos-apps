{
  "name": "@apollosproject/ui-prayer",
  "version": "1.4.2",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "test": "TZ=UTC jest --no-watchman",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "peerDependencies": {
    "@apollo/react-hooks": "*",
    "@apollosproject/storybook": "*",
    "@apollosproject/ui-analytics": "*",
    "@apollosproject/ui-fragments": "*",
    "@apollosproject/ui-kit": "*",
    "@apollosproject/ui-notifications": "*",
    "@apollosproject/ui-scripture": "*",
    "react": "*",
    "react-apollo": "*",
    "react-native": "*",
    "react-navigation": "*"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@react-navigation|@apollosproject|@apollosproject/ui-*))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "modulePathIgnorePatterns": [
      "node_modules/@storybook/"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "dependencies": {
    "formik": "1.5.1",
    "graphql-tag": "2.9.2",
    "lodash": "4.17.13",
    "react-is": "^16.8.6",
    "react-native-swiper": "^1.6.0",
    "yup": "0.26.6"
  },
  "devDependencies": {
    "apollo-client": "2.6.10",
    "@apollo/react-testing": "3.1.4",
    "@apollo/react-hooks": "3.1.5",
    "@storybook/react-native": "5.2.4",
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-navigation": "^3.11.0",
    "react-test-renderer": "16.8.6",
    "waait": "1.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

{
  "name": "@apollosproject/rock-apollo-data-source",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "version": "2.0.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcovonly",
      "clover"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "@apollosproject/config": "*"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.6.4",
    "apollo-server-env": "^2.3.0",
    "lodash": "^4.17.11",
    "with-query": "^1.1.2"
  },
  "devDependencies": {
    "@apollosproject/babel-preset-apollos": "^2.0.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

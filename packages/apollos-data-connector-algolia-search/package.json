{
  "name": "@apollosproject/data-connector-algolia-search",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "engines": {
    "node": "10.x.x"
  },
  "version": "2.2.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcovonly",
      "clover"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@apollosproject/server-core": "^2.2.0",
    "algoliasearch": "^3.34.0",
    "apollo-datasource-rest": "^0.6.4"
  },
  "peerDependencies": {
    "@apollosproject/config": "*",
    "@apollosproject/data-schema": "*"
  },
  "devDependencies": {
    "@apollosproject/apollo-server-env-mock": "^2.2.0",
    "@apollosproject/babel-preset-apollos": "^2.2.0",
    "apollo-server-env": "^2.3.0",
    "graphql": "14.7.0",
    "jest": "^25.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

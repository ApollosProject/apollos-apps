{
  "name": "@apollosproject/ui-mapview",
  "version": "1.4.1",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "test": "jest --no-watchman",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@apollosproject))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "peerDependencies": {
    "@apollosproject/config": "*",
    "@apollosproject/storybook": "*",
    "@apollosproject/ui-fragments": "*",
    "@apollosproject/ui-kit": "*",
    "@apollosproject/ui-media-player": "*",
    "@react-native-community/geolocation": "*",
    "react": "*",
    "react-native": "*",
    "react-native-geolocation-service": "*",
    "react-native-maps": "*",
    "react-navigation": "*"
  },
  "dependencies": {
    "graphql-tag": "2.9.2",
    "react-apollo": "2.5.5"
  },
  "devDependencies": {
    "@react-native-community/geolocation": "^2.0.2",
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-geolocation-service": "^3.0.0",
    "react-native-maps": "0.26.1",
    "react-navigation": "^3.11.0",
    "react-test-renderer": "16.6.3",
    "waait": "1.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

{
  "name": "@apollosproject/data-connector-passes",
  "repository": "git://github.com/apollosproject/apollos-apps.git",
  "engines": {
    "node": ">=12"
  },
  "version": "2.20.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcovonly",
      "clover"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@apollosproject/server-core": "^2.20.0",
    "apollo-datasource": "^0.4.0",
    "datauri": "^1.1.0",
    "lodash": "^4.17.11",
    "passkit-generator": "1.5.9",
    "qrcode": "^1.3.3"
  },
  "peerDependencies": {
    "@apollosproject/config": "*",
    "@apollosproject/data-connector-rock": "*",
    "@apollosproject/data-schema": "*",
    "apollo-server": "^2.11.0"
  },
  "devDependencies": {
    "@apollosproject/apollo-server-env-mock": "^2.20.0",
    "@apollosproject/babel-preset-apollos": "^2.20.0",
    "apollo-server-env": "^2.3.0",
    "graphql": "14.7.0",
    "graphql-tag": "^2.10.1",
    "jest": "^25.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}

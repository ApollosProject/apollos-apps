{
  "name": "@apollosproject/ui-scripture",
  "version": "1.1.0-beta.2",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "test": "jest",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@apollosproject))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "modulePathIgnorePatterns": [
      "node_modules/@storybook/"
    ]
  },
  "peerDependencies": {
    "@apollos-project/ui-htmlview": "*",
    "@apollos-project/ui-kit": "*",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@apollosproject/ui-storybook": "^1.1.0-beta.2",
    "@storybook/react-native": "^4.0.7",
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "prop-types": "15.6.1",
    "react": "^16.8.3",
    "react-native": "0.59.10",
    "react-test-renderer": "16.6.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}
